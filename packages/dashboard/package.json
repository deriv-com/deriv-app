{
    "name": "@deriv/dashboard",
    "version": "0.0.4",
    "description": "Deriv dashboard",
    "main": "lib/index.js",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "lib"
    ],
    "scripts": {
        "test:typecheck": "tsc --noEmit --preserveWatchOutput",
        "build": "rimraf lib && webpack --progress",
        "build:publish": "npm run publish_prepare; deriv-publisher postpublish",
        "build:travis": "rimraf lib && webpack",
        "serve": "rimraf lib && concurrently \"cross-env BUILD_MODE='serve' webpack --progress --watch\" \"tsc -w --noEmit --preserveWatchOutput\"",
        "start": "rimraf lib && npm run test && npm run serve",
        "deploy": "echo \"No deploy specified\"",
        "deploy:clean": "echo \"No deploy:clean specified\"",
        "deploy:folder": "echo \"No deploy:folder specified\"",
        "deploy:staging": "echo \"No deploy:staging specified\"",
        "deploy:production": "echo \"No deploy:production specified\"",
        "publish_package": "npm run publish_prepare && npm publish; deriv-publisher postpublish",
        "publish_prepare": "deriv-publisher prepublish && lerna exec --scope=@deriv/shared --scope=@deriv/components -- npm run build:travis -- && rimraf lib && cross-env NODE_ENV=production NPM_PUBLISHING_MODE=1 webpack --progress",
        "test:eslint": "eslint \"./src/**/*.?(ts|tsx)\""
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@deriv/account": "^1.0.0",
        "@deriv/components": "^1.0.0",
        "@deriv/shared": "^1.0.0",
        "@deriv/trader": "^3.8.0",
        "@deriv/translations": "^1.0.0",
        "classnames": "^2.2.6",
        "mobx": "^5.15.7",
        "mobx-react-lite": "^2.2.2",
        "object.fromentries": "^2.0.0",
        "prop-types": "^15.7.2",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-proposal-decorators": "^7.12.12",
        "@babel/plugin-proposal-export-default-from": "^7.12.1",
        "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
        "@babel/plugin-proposal-optional-chaining": "^7.12.7",
        "@babel/plugin-proposal-unicode-property-regex": "^7.12.1",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-react": "^7.12.10",
        "@babel/preset-typescript": "^7.12.7",
        "@deriv/api-types": "1.0.17",
        "@deriv/publisher": "^0.0.1-beta4",
        "@types/classnames": "^2.2.11",
        "@types/object.fromentries": "^2.0.0",
        "@types/react-router-dom": "^5.1.6",
        "@typescript-eslint/eslint-plugin": "^4.10.0",
        "@typescript-eslint/parser": "^4.10.0",
        "babel-core": "^6.26.3",
        "babel-jest": "^26.5.2",
        "babel-loader": "^8.1.0",
        "chai": "^4.2.0",
        "concurrently": "^5.3.0",
        "cross-env": "^5.2.0",
        "css-loader": "^5.0.1",
        "css-modules-typescript-loader": "^4.0.1",
        "lint-staged": "^10.4.0",
        "mini-css-extract-plugin": "^1.3.4",
        "node-sass": "^4.12.0",
        "optimize-css-assets-webpack-plugin": "^5.0.4",
        "postcss-loader": "^4.1.0",
        "postcss-preset-env": "^6.7.0",
        "postcss-scss": "^3.0.4",
        "resolve-url-loader": "^3.1.2",
        "sass-loader": "^10.1.1",
        "sass-resources-loader": "^2.1.1",
        "source-map-loader": "^1.1.2",
        "style-loader": "^1.2.1",
        "terser-webpack-plugin": "^5.1.1",
        "typescript": "^4.1.3",
        "webpack": "^5.14.0",
        "webpack-bundle-analyzer": "^4.3.0",
        "webpack-cli": "^4.3.1"
    }
}

    "formik": "^2.1.4",
    "js-cookie": "^2.2.1",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "onfido-sdk-ui": "^5.12.0",
    "prop-types": "^15.7.2",
    "qrcode.react": "^1.0.0",
    "react": "^16.13.1",
    "react-loadable": "^5.5.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },

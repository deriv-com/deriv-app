{
  "name": "@deriv/components",
  "version": "1.0.0",
  "description": "deriv components",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "start": "rimraf lib && npm run test && npm run serve",
    "serve": "rimraf lib && cross-env BUILD_MODE='serve' webpack --mode=production --progress --watch",
    "build": "rimraf lib && npm run build:icons && webpack --mode=production --optimize-minimize --progress",
    "build:icons": "node -e 'require(\"./utils/build-icons\").buildIcons()' && npm run prettify",
    "build:travis": "rimraf lib && webpack --mode=production --optimize-minimize",
    "prettify": "prettier --write \"./{utils,src,stories}/**/*.{js,jsx,scss}\"",
    "precommit": "lint-staged",
    "test": "npm run test:eslint && npm run test:stylelint",
    "test:eslint": "eslint --fix \"./src/**/*.?(js|jsx)\"",
    "test:stylelint": "stylelint --fix \"./src/**/*.s?(a|c)ss\"",
    "deploy": "echo \"No deploy specified\"",
    "deploy:clean": "echo \"No deploy:clean specified\"",
    "deploy:folder": "echo \"No deploy:folder specified\"",
    "deploy:staging": "echo \"No deploy:staging specified\"",
    "deploy:production": "echo \"No deploy:production specified\"",
    "storybook": "start-storybook -s /.out -p 6006",
    "storybook:build": "build-storybook --output-dir .out",
    "storybook:deploy": "rm -fr .out && npm run storybook:build && gh-pages -d .out -e $1"
  },
  "author": "Deriv",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-info": "^5.2.4",
    "@storybook/addon-links": "^5.2.4",
    "@storybook/addon-notes": "^5.2.4",
    "@storybook/addon-viewport": "^5.3.9",
    "@storybook/addons": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "@storybook/source-loader": "^5.2.4",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-binary": "^1.0.2",
    "eslint-config-prettier": "^6.0.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "lint-staged": "^10.0.6",
    "node-sass": "^4.12.0",
    "prettier": "1.18.2",
    "sass-loader": "^9.0.2",
    "sass-resources-loader": "^2.0.3",
    "style-loader": "^1.2.1",
    "stylelint": "^10.0.1",
    "stylelint-formatter-pretty": "^1.0.3",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "svg-sprite-loader": "^4.1.6",

{
  "name": "@deriv/components",
  "version": "1.0.0",
  "description": "deriv components",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "start": "rimraf lib && npm run test && npm run serve",
    "serve": "rimraf lib && cross-env BUILD_MODE='serve' webpack --mode=production --progress --watch",
    "build": "rimraf lib && npm run build:icons && webpack --mode=production --progress",
    "build:icons": "node -e 'require(\"./utils/build-icons\").buildIcons()'",
    "build:travis": "rimraf lib && webpack --mode=production",
    "test": "node \"./src/components/icon/__tests__/icons.js\"",
    "test:eslint": "eslint \"./src/**/*.?(js|jsx)\"",
    "deploy": "echo \"No deploy specified\"",
    "deploy:clean": "echo \"No deploy:clean specified\"",
    "deploy:folder": "echo \"No deploy:folder specified\"",
    "deploy:staging": "echo \"No deploy:staging specified\"",
    "deploy:production": "echo \"No deploy:production specified\"",
    "storybook": "start-storybook -s /.out -p 6006",
    "storybook:build": "build-storybook --output-dir .out",
    "storybook:deploy": "rm -fr .out && npm run storybook:build && gh-pages -d .out -e $1"
  },
  "author": "Deriv",
  "license": "Apache-2.0",
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-info": "^5.2.4",
    "@storybook/addon-links": "^5.2.4",
    "@storybook/addon-notes": "^5.2.4",
    "@storybook/addon-viewport": "^5.3.9",
    "@storybook/addons": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "@storybook/source-loader": "^5.2.4",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.2.0",
    "cross-env": "^5.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-binary": "^1.0.2",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob": "^7.1.5",
    "lint-staged": "^10.4.0",
    "node-sass": "^4.12.0",
    "react": "^16.13.1",
    "sass-loader": "^10.1.1",
    "sass-resources-loader": "^2.1.1",
    "style-loader": "^1.2.1",
    "svg-sprite-loader": "^5.2.1",
    "svgo-loader": "^2.2.0",
    "webpack": "^5.14.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1"
  },
  "dependencies": {
    "@contentpass/zxcvbn": "^4.4.3",
    "@deriv/shared": "^1.0.0",
    "@storybook/addon-knobs": "^5.2.4",
    "@wapps/storybook-addon-state": "0.0.4",
    "classnames": "^2.2.6",
    "gh-pages": "^2.1.1",
    "lodash.throttle": "^4.1.1",
    "prop-types": "^15.7.2",
    "react-content-loader": "^4.3.2",
    "react-div-100vh": "^0.3.8",
    "react-dom": "^16.13.1",
    "react-drag-drawer": "^3.3.4",
    "react-dropzone": "11.0.1",
    "react-pose": "^4.0.10",
